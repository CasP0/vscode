{
	"comments": {
		"lineComment": "REM",
		"blockComment": ["/*", "*/"]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["<", ">"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["<", ">"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["<", ">"]
	],
	"folding": {
		"markers": {
			"start": "^\\s*(::\\s*|REM\\s+)#region",
			"end": "^\\s*(::\\s*|REM\\s+)#endregion"
		}
	}
}
